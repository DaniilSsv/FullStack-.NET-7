@model IEnumerable<FullstackOpdracht.ViewModels.TeamVM>

@{
    ViewData["Title"] = "Index";
}
<table class="table">
    <thead>
        <tr>
            
            <th>
                Team
            </th>
            
            <th>
                Prijs
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                 <a asp-controller="Subscription" asp-action="CreateAbonnement" asp-route-id="@item.Id"><button>Bestel dit abonnement!</button></a>
            </td>
        </tr>
}
    </tbody>
</table>
